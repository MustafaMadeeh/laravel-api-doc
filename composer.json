{
  "name": "madeeh/for-api-doc",
  "description": "A Laravel package for API documentation",
  "keywords": ["laravel", "api", "documentation"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mustafa Madeeh",
      "email": "mustafamadeeh9@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^9.0|^10.0|^11.0",
    "madeeh/for-api-doc": "dev-main"
  },
  "autoload": {
    "psr-4": {
     "ApiDocs\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Madeeh\\ApiDocs\\Providers\\ApiDocsServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/MustafaMadeeh/for-api-doc"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
