{
    "name": "madeeh/for-api-doc",
    "description": "A Laravel package for API documentation",
    "keywords": ["laravel", "api", "documentation"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mustafa Madeeh",
            "email": "mustafamadeeh9@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
"autoload": {
    "psr-4": {
        "Madeeh\\ApiDocs\\": "src/"
    }
},
"extra": {
    "laravel": {
        "providers": [
            "MyApiDocs\\Providers\\ApiDocsServiceProvider"
        ]
    }
}

    "minimum-stability": "dev",
    "prefer-stable": true
}
